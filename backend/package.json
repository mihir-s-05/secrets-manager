{
  "name": "@secrets-manager/backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run --maxWorkers=1 --minWorkers=1",
    "test:watch": "vitest --maxWorkers=1 --minWorkers=1",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/sensible": "^5.0.1",
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.2",
    "prisma": "^5.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "supertest": "^6.3.4",
    "tsx": "^4.19.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
